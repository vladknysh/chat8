<div id="chatroom_container" class="row bg-light text-dark">
  <div id="side_panel" class="col-md-4 col-sm-4">
    <h5> <%= current_user.email %> </h5>
    <h4> Users </h4>
    <%= turbo_stream_from "users" %>
    <div id="users">
      <%= render @users %>
    </div>
    <h4> Rooms </h4>
    <%= turbo_stream_from "rooms" %>
    <%#= render 'layouts/new_room_form' %>
    <div id="rooms">
      <%= render @rooms%>
    </div>
  </div>
</div>